{
  "name": "mable",
  "version": "0.4.0",
  "description": "A strongly-typed, functional toolbelt for TypeScript and JavaScript",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/index.js",
    "dist/index.mjs",
    "dist/index.d.ts"
  ],
  "keywords": [
    "strong",
    "type",
    "functional",
    "programming",
    "typescript",
    "maybe",
    "remotedata",
    "result"
  ],
  "eslintConfig": {
    "extends": "./node_modules/@bakerface/typescript-starter/eslint",
    "rules": {
      "@typescript-eslint/no-explicit-any": "off"
    }
  },
  "nyc": {
    "extends": "@bakerface/typescript-starter/nyc"
  },
  "scripts": {
    "prepare": "typescript-starter build src/index.ts",
    "lint": "typescript-starter lint .",
    "test": "typescript-starter test 'src/**/*.test.*'"
  },
  "devDependencies": {
    "@bakerface/typescript-starter": "^0.5.4"
  }
}
